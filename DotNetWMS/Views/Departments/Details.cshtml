@model DotNetWMS.Models.Department

@{
    ViewData["Title"] = "Details";
}



<h4>Szczegóły stanowiska</h4>
<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-success"><i class="fas fa-edit"></i> Edytuj</a>
<hr />
<dl class="row details-view">
    <dt class="col-sm-2 list-group-item">
        @Html.DisplayNameFor(model => model.Name)
    </dt>
    <dd class="col-sm-10 list-group-item dd-list">
        @Html.DisplayFor(model => model.Name)
    </dd>
</dl>


<a asp-action="Index" class="btn btn-outline-dark"><i class="fas fa-arrow-left"></i>Wróć do podglądu</a>

